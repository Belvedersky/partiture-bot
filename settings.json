  "sql": {
    "initSession": "CREATE TABLE Session (id VARCHAR(255) PRIMARY KEY, session VARCHAR(255));",
    "initVoice": "CREATE TABLE Voices (id INTEGER PRIMARY KEY AUTOINCREMENT, voice TEXT, image TEXT, duration INT, username TEXT, downloaded INTEGER DEFAULT(0));",
    "initPartiture": "CREATE TABLE Paritures (id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, image TEXT NOT NULL, chatId TEXT NOT NULL, username TEXT, description TEXT, anonymous INT DEFAULT(1), moderated INT DEFAULT(0) )",
    "addVoice": "INSERT INTO Voices (voice,image,duration) VALUES (?,?,?)",
    "addAuthorVoice": "UPDATE Voices SET username = ? WHERE voice = ?;",
    "addPartiture": "INSERT INTO Paritures (image, chatId,  username, description) VALUES (?,?,?,?)",
    "randomVoice": "SELECT * FROM Voices WHERE image IS NOT NULL ORDER BY RANDOM() LIMIT 1;",
    "randomPartiture": "SELECT * FROM Paritures ORDER BY RANDOM() LIMIT 1;"
